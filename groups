question==>https://codeforces.com/contest/1598/problem/B

#include<bits/stdc++.h>
using namespace std;
#define int long long 
signed main(){
    int t;
    cin>>t;
    while(t--){
        int n;
        cin>>n;
        int a[n][5];
        for(int i = 0;i<n;i++){
            for(int j = 0;j<5;j++){
                cin>>a[i][j];
            }
        }
        bool result = false;
        for(int i = 0;i<4;i++){
            for(int j = i+1;j<5;j++){
                int d1=0,d2=0,both=0;
                for(int k = 0;k<n;k++){
                    if(a[k][i]&&a[k][j])both++;
                    else if(a[k][i])d1++;
                    else if(a[k][j])d2++;
                }
                int need1 = n/2-d1;
                int need2 = n/2-d2;
                if(need1>=0 && need2>=0 && both==need1+need2){
                    result=true;
                    break;
                }
            }
            if(result)break;
        }
        if(result)cout<<"YES\n";
        else cout<<"NO\n";
    }
    return 0;
}
