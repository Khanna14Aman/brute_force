question==>https://codeforces.com/problemset/problem/459/A


#include<bits/stdc++.h>
#define int long long 
using namespace std;
signed main(){
    int x1,y1,x2,y2;
    cin>>x1>>y1>>x2>>y2;
    if(x1==x2){
        cout<<x1+abs(y2-y1)<<" "<<y1<<" "<<x2+abs(y2-y1)<<" "<<y2<<endl;
        return 0;
    }
    if(y1==y2){
        cout<<x1<<" "<<y1+abs(x2-x1)<<" "<<x2<<" "<<y2+abs(x2-x1)<<endl;
        return 0;
    }
    if(abs(y2-y1)==abs(x2-x1)){
        cout<<x1<<" "<<y2<<" "<<x2<<" "<<y1<<endl;
        return 0;
    }
    cout<<"-1\n";
    return 0;
}
